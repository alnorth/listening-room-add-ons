{
	"name": "Listening Room Add-ons",
	"version": "0.10.5",
	"description": "Charts, UI enhancements and scrobbling for Listening Room.",
	"background_page": "background.html",
	"content_scripts": [
		{
		"matches": ["http://listeningroom.net/room/*", "http://www.listeningroom.net/room/*", "http://listeningroom.fettig.net/room/*"],
		"css": ["content.css"],
		"js": ["constants.js", "lrdata-interface.js", "charts.js", "content.js"]
		}
	],
	"permissions": [
		"tabs", "notifications"
	],
	"icons": {
		"16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
	}
}